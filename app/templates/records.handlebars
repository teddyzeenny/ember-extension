<div class="table-tree">
  <div class="table-tree__table-container">
    <table cellspacing="0" border-collapse="collapse">
      <thead>
        <tr>
          {{#each attributes}}
            <th><div class="table-tree__th-inner">{{name}}</div></th>
          {{/each}}
        </tr>
      </thead>
      <tbody>
        {{#each model itemController="record"}}
          <tr data-label="model-type-row" {{action "inspectModel" model}}>
            {{#each attributes}}
            <td data-label="model-type-name" class="table-tree__clickable" >
              {{value}}
            </td>
            {{/each}}
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
