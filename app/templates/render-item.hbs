<div data-label="render-profile-row" class="list-tree__item-wrapper row-wrapper">
  <div style={{nodeStyle}} class="list-tree__item row {{expandedClass}}"  data-label="render-profile-item">
    <div class="cell_type_main cell" {{action "toggleExpand"}} style={{nameStyle}} data-label="render-main-cell">

      <span title="{{model.name}}">
        <span class="cell__arrow"></span>
        <span data-label="render-profile-name">{{model.name}}</span>
        <span class="pill pill_not-clickable" data-label="render-profile-duration">{{ms-to-time model.duration}}</span>
      </span>
    </div>
    <div class="cell cell_value_numeric"  data-label="render-profile-timestamp">
      {{readableTime}}
    </div>
  </div>
</div>
{{#if isExpanded}}
  {{#each model.children as |child|}}
    {{render "render_item" child}}
  {{/each}}
{{/if}}
