<tr {{bind-attr class="selected:table-tree__row_selected"}} data-label="model-type-row" {{action "viewRecords" this}}>

  <td data-label="model-type-name" class="table-tree__clickable" >
    {{name}}
  </td>

  {{#unless collapsed}}
  <td data-label="model-type-count" class="table-tree__clickable" >
    {{count}}
  </td>
  {{/unless}}
</tr>
